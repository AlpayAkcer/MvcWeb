
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Limitless - Responsive Web Application Kit by Eugene Kopyov</title>
    <!-- Global stylesheets -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">
    <link href="/Theme/Admin/assets/css/icons/icomoon/styles.css" rel="stylesheet" type="text/css">
    <link href="/Theme/Admin/assets/css/minified/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="/Theme/Admin/assets/css/minified/core.min.css" rel="stylesheet" type="text/css">
    <link href="/Theme/Admin/assets/css/minified/components.min.css" rel="stylesheet" type="text/css">
    <link href="/Theme/Admin/assets/css/minified/colors.min.css" rel="stylesheet" type="text/css">
