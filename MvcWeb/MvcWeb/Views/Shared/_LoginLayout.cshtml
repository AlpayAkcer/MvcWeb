<body>
    <!-- Main navbar -->
    <div class="navbar navbar-inverse">
        <div class="navbar-header">
            <a class="navbar-brand" href="/Login/AdminGiris">Admin Yönetim Paneli</a>
            <ul class="nav navbar-nav pull-right visible-xs-block">
